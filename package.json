{
  "name": "blog-frame",
  "version": "1.0.0",
  "description": "Blog with Gatsby and Typescript",
  "author": "siner308 <siner308@gmail.com>",
  "license": "MIT",
  "scripts": {
    "update-posts": "git submodule update --remote",
    "start": "npm run build && npm run serve",
    "develop": "gatsby develop",
    "build": "npm run update-posts && gatsby build",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
    "test": "jest"
  },
  "sideEffects": false,
  "dependencies": {
    "@deckdeckgo/highlight-code": "^4.1.0",
    "gatsby": "^4.9.3",
    "gatsby-plugin-anchor-links": "^1.2.1",
    "gatsby-plugin-google-adsense": "^1.1.3",
    "gatsby-plugin-google-analytics": "^4.9.0",
    "gatsby-plugin-google-tagmanager": "^4.9.0",
    "gatsby-plugin-postcss": "^5.9.0",
    "gatsby-plugin-react-helmet": "^5.9.0",
    "gatsby-plugin-typography": "^4.9.0",
    "gatsby-remark-highlight-code": "^3.2.0",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-filesystem": "^4.9.1",
    "gatsby-transformer-remark": "^5.9.1",
    "postcss": "^8.4.8",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.20",
    "typography": "^0.16.21"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-helmet": "^6.1.5",
    "@types/typography": "^0.16.4",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2"
  }
}
