{
  "name": "blog-frame",
  "version": "1.0.0",
  "description": "Blog with Gatsby and Typescript",
  "author": "siner308 <siner308@gmail.com>",
  "license": "MIT",
  "scripts": {
    "update-posts": "git submodule update --remote",
    "start": "npm run build && npm run serve",
    "develop": "gatsby develop",
    "build": "npm run update-posts && gatsby build",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
    "test": "jest"
  },
  "sideEffects": false,
  "dependencies": {
    "@deckdeckgo/highlight-code": "^2.2.1",
    "gatsby": "next",
    "gatsby-plugin-typography": "next",
    "gatsby-remark-highlight-code": "^2.1.0",
    "gatsby-source-filesystem": "^2.6.1",
    "gatsby-transformer-remark": "^2.11.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/typography": "^0.16.3",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unused-imports": "^1.0.1",
    "jest": "^26.6.3",
    "prettier": "2.1.1",
    "typescript": "^4.1.2"
  }
}
