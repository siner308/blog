    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unused-imports": "^1.0.1",
    "jest": "^26.6.3",
    "prettier": "2.1.1",
    "typescript": "^4.1.2"
